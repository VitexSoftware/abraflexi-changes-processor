{
  "name": "vitexsoftware/abraflexi-changes-processor",
  "description": "AbraFlexi Changes Proscessor",
  "version": "0.1",
  "type": "project",
  "require": {
    "rogervila/array-diff-multidimensional": "*",
    "deb/abraflexi": "*",
    "deb/ease-bricks": "*",
    "deb/abraflexi-bricks": "*",
    "deb/ease-fluentpdo": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "config": {
    "vendor-dir": "/var/lib/abraflexi-changes-processor"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "AbraFlexi\\Processor\\": "./Processor/",
      "AbraFlexi\\Processor\\Notify\\": "./Processor/Notify/",
      "AbraFlexi\\Processor\\Ui\\": "./Processor/Ui/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseHtml/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseBricks/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexi/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexiBricks/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseFluentPDO/"
    }
  ]
}
